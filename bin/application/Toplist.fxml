<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="226.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ToplistController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1080.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bgimage2.jpg" />
         </image>
      </ImageView>
      <MenuBar prefHeight="29.0" prefWidth="1080.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem fx:id="exit" mnemonicParsing="false" onAction="#exitProgram" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Button fx:id="AdvSearch" defaultButton="true" layoutX="103.0" layoutY="153.0" mnemonicParsing="false" onAction="#AdvSearch" opacity="0.86" text="Advanced Search" textOverrun="CENTER_ELLIPSIS">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <Button fx:id="MyBooks" defaultButton="true" layoutX="338.0" layoutY="153.0" mnemonicParsing="false" onAction="#EnterMyBorrowedBooks" opacity="0.86" text="My Borrowed Books" textOverrun="CENTER_ELLIPSIS">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <Button fx:id="Toplist" defaultButton="true" layoutX="577.0" layoutY="153.0" mnemonicParsing="false" onAction="#GoToToplist" opacity="0.86" text="Toplist" textOverrun="CENTER_ELLIPSIS">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <Button fx:id="CheckOut" defaultButton="true" layoutX="778.0" layoutY="153.0" mnemonicParsing="false" onAction="#GoToCheckOut" opacity="0.86" text="Check Out" textOverrun="CENTER_ELLIPSIS">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <Button fx:id="GoBack" defaultButton="true" layoutX="42.0" layoutY="93.0" mnemonicParsing="false" onAction="#GoBack" opacity="0.86" text="Go Back" textOverrun="CENTER_ELLIPSIS">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <TableView fx:id="result" layoutX="181.0" layoutY="265.0" prefHeight="345.0" prefWidth="718.0">
         <columns>
            <TableColumn fx:id="TitleCol" prefWidth="135.0" text="Title" />
            <TableColumn fx:id="AuthorCol" prefWidth="133.0" text="Author" />
            <TableColumn fx:id="GenreCol" prefWidth="103.0" text="Genre" />
            <TableColumn fx:id="PagesCol" prefWidth="51.0" text="Pages" />
            <TableColumn fx:id="PublisherCol" prefWidth="143.0" text="Publisher" />
            <TableColumn fx:id="QuantityCol" prefWidth="76.0" text="Quantity" />
            <TableColumn fx:id="ISBNCol" prefWidth="76.0" text="ISBN" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
